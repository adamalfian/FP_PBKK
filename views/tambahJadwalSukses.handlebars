<form class="form-horizontal" action="/tambahjadwal" method="POST">
    <div class="form-group">
      <label class="control-label col-sm-2">Kode Mata Kuliah:</label>
      <div class="col-sm-10">
        <input type="text" class="form-control" placeholder="Enter kode mata kuliah" name="fk_kode_mk">
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2">Pertemuan ke:</label>
      <div class="col-sm-10">          
        <input type="text" class="form-control"  placeholder="Enter pertemuan" name="pertemuan">
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2">Jam Masuk:</label>
      <div class="col-sm-10">          
        <input type="text" class="form-control"  placeholder="Enter jam masuk" name="jam_masuk">
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2">Jam Pulang:</label>
      <div class="col-sm-10">          
        <input type="text" class="form-control"  placeholder="Enter jam pulang" name="jam_pulang">
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2">Ruang:</label>
      <div class="col-sm-10">          
        <input type="text" class="form-control"  placeholder="Enter nama ruang" name="ruang">
      </div>
    </div>
    <div class="form-group">        
      <div class="col-sm-offset-2 col-sm-10">
        <button type="submit" class="btn btn-default">Submit</button>
      </div>
