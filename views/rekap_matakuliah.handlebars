<h3><center> rekap mata kuliah {{kode_mk}}</center> </h3>
<table class="table table-hover">
    <thead>
      <tr>
        <th>Mahasiswa</th>
        <th>Nama Mata Kuliah</th>
        <th>Pertemuan</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody>
      {{#each rekap}}
        <tr>
            <td>{{nama}}</td>
            <td>{{nama_matkul}}</td>
            <td>{{pertemuan_ke}}</td>
            <td>{{status}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>